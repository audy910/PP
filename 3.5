
import java.util.*;

public class intValidation {
    public static void main(String args[]) 
    {
      int input = 6;
      Scanner scan = new Scanner(System.in);
      while (input < 10)
      {
        System.out.println("Enter an integer greater than 10:");
        input = scan.nextInt();

      }//end while
      System.out.println("Thank you");
    }//end main
}// end intValidation
